{
  "name": "mega-wedding",
  "private": true,
  "description": "Website for the wedding of Hailey Zislis and Sam Gaus",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "lint": "eslint .",
    "format": "prettier --write -- 'app/**/*.{ts,tsx}'",
    "start": "remix-serve build",
    "generate-types": "export $(cat .env) && openapi-typescript \"${SUPABASE_URL}/rest/v1/?apikey=${SUPABASE_ANON_KEY}\" --version=2 --output app/types/database.ts",
    "dump-schema": "export $(cat .env) && export PGPASSWORD=$DATABASE_PASSWORD && pg_dump --schema=public --quote-all-identifiers --schema-only -h $DATABASE_HOST -U postgres > supabase.sql"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.2.1",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11",
    "@emotion/server": "^11",
    "@emotion/styled": "^11",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@remix-run/node": "^1.3.5",
    "@remix-run/react": "^1.3.5",
    "@remix-run/serve": "^1.3.5",
    "@supabase/supabase-js": "^1.35.3",
    "datebook": "^7.0.8",
    "dotenv": "^16.0.1",
    "framer-motion": "^6",
    "mapbox-gl": "^2.8.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-map-gl": "^7.0.15",
    "telegraf": "^4.8.5"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.5",
    "@remix-run/eslint-config": "^1.3.5",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "openapi-typescript": "^5.4.0",
    "prettier": "^2.6.2",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
